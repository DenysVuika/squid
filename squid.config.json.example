{
  "api_url": "http://127.0.0.1:1234/v1",
  "api_model": "local-model",
  "log_level": "error",
  "enable_env_context": true,
  "permissions": {
    "allow": [
      "now",
      "read_file",
      "grep",
      "bash:ls",
      "bash:pwd",
      "bash:git status",
      "bash:cat"
    ],
    "deny": []
  },
  "rag": {
    "enabled": true,
    "embedding_model": "text-embedding-nomic-embed-text-v1.5",
    "embedding_url": "http://127.0.0.1:11434",
    "chunk_size": 512,
    "chunk_overlap": 50,
    "top_k": 5,
    "documents_path": "documents"
  },
  "version": "0.6.0"
}
